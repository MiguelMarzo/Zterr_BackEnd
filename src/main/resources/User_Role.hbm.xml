<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- Mapping configuration details between User_Role class and User -->
<hibernate-mapping package="org.zterr.data.model">

	<class name="User_Role" table="user_role">
			<id name="id" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="idUser" column="iduser" />
		<property name="idRole" column="idrole" />
	</class>

</hibernate-mapping>